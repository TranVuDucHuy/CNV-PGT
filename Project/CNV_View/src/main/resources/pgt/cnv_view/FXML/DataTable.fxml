<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="root" prefWidth="1030.0" prefHeight="600.0" maxWidth="Infinity" maxHeight="Infinity" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pgt.cnv_view.Controller.DataTable">
   <top>
   <AnchorPane fx:id="header" prefHeight="25.0" style="-fx-background-color: #ced1f5;" BorderPane.alignment="CENTER">
         <children>
            <Label text="Data Table" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
   </top>
   <center>
      <TableView fx:id="table" BorderPane.alignment="CENTER" prefWidth="1030.0" maxHeight="Infinity">
         <columns>
            <TableColumn fx:id="colChrom" prefWidth="90.0" text="Chromosome" />
            <TableColumn fx:id="colStart" prefWidth="110.0" text="Start" />
            <TableColumn fx:id="colEnd" prefWidth="110.0" text="End" />
            <TableColumn fx:id="colCopy" prefWidth="110.0" text="Copy number" />
            <TableColumn fx:id="colType" prefWidth="110.0" text="Type" />
         </columns>
      </TableView>
   </center>
</BorderPane>
